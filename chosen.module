<?php
/**
 * Implementation of hook_init()
 */
function chosen_init() {
  drupal_add_js(drupal_get_path('module', 'chosen') . '/chosen/chosen.jquery.js');
  drupal_add_css(drupal_get_path('module', 'chosen') . '/chosen/chosen.css');
  drupal_add_js(drupal_get_path('module', 'chosen') . '/includes/chosen.js');
}